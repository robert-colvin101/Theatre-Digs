<div class="new-section">
  <%= simple_form_for(@property) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.submit %>
<% end %>
</div>
